					"listen": "test",
					"script": {
						"exec": [
							"pm.environment.set('cartId', pm.response.json().cartId);\r",
							"pm.test(\"Item by ID is added to cart\", function () {\r",
							"    const response = pm.response.json();\r",
							"    \r",
							"    // Check if items array exists\r",
							"    pm.expect(response.items).to.exist;\r",
							"    pm.expect(response.items).to.be.an('array');\r",
							"\r",
							"    // Validate each item in the cart\r",
							"    response.items.forEach(item => {\r",
							"        pm.expect(item).to.have.property('itemId');\r",
							"        pm.expect(item).to.have.property('quantity');\r",
							"        \r",
							"        // Check if the itemId matches the expected itemId\r",
							"        pm.expect(item.itemId).to.equal(pm.environment.get('itemId'));\r",
							"    });\r",
