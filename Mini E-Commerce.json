					"script": {
						"exec": [
							"let items = pm.response.json();",
							"if (Array.isArray(items) && items.length > 0) {",
							"    pm.environment.set(\"itemId\", items[0].id);",
							"    pm.environment.set(\"itemId2\", items[1]?.id || \"\");",
							"    pm.environment.set(\"itemId3\", items[2]?.id || \"\");",
							"    pm.environment.set(\"itemId4\", items[3]?.id || \"\");",
							"}",
							"pm.test(\"Response is JSON\", function () {",
							"    pm.response.to.be.withBody;",
							"    pm.response.to.be.json;",
							"});",
							"pm.test(\"Items are saved\", function () {",
							"    pm.expect(pm.response.code).to.equal(200);",
							"    const response = pm.response.json();",
							"    pm.expect(response).to.be.an('array').that.is.not.empty;",
							"",
							"    response.forEach(item => {",
							"        pm.expect(item).to.have.property('id').that.is.a('string');",
							"        pm.expect(item).to.have.property('name').that.is.a('string');",
							"        pm.expect(item).to.have.property('price').that.is.a('number');",
							"    });",
							"});",
